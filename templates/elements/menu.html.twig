{{ wrapper.open() }}
    {% for item in items %}
        <li class="{{ item.class }}">
            {% set link_class = item.current ? item.link_class ~ ' active' : item.link_class %}
            <a href="{{ item.url }}" class="{{ link_class }}">{{- t(item.label) -}}</a>
        </li>
    {% endfor %}
{{ wrapper.close() }}